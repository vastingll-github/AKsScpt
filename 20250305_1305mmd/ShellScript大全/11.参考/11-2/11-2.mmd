      echo &quot;start&quot; # you can write code here
      %text
      #|aaa
      #|bbb
      #|ccc
      echo &quot;end&quot; # you can write code here
    }

    result() { %text
      #|start
      #|aaa
      #|bbb
      #|ccc
      #|end
    }

    When call output
    The output should eq &quot;$(result)&quot;
    The line 3 of output should eq 'bbb'
  End
End">

    Describe '%text directive'
      It 'outputs texts'
        output() {
          echo "start" # you can write code here
          %text
          #|aaa
          #|bbb
          #|ccc
          echo "end" # you can write code here
        }

        result() { %text
          #|start
          #|aaa
          #|bbb
          #|ccc
          #|end
        }

        When call output
        The output should eq "$(result)"
        The line 3 of output should eq 'bbb'
      End
    End

</div>

